{
    "name": "romain/ski-cup-logitud",
    "authors": [
        {
            "name": "Romain Balestreri"
        }
    ],
    "description": "Ski championship manager application",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nesbot/carbon": "^2.41",
        "twig/twig": "^3.0",
        "symfony/http-foundation": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/config": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.10",
        "pestphp/pest": "^0.3.9",
        "spatie/phpunit-watcher": "^1.22",
        "symfony/var-dumper": "^5.1"
    }
}
